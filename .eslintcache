rs/USER/Desktop/nodejs/mern-ecommerce-front/src/core/Home.js",["92"],"import React, { useState, useEffect } from 'react';\nimport Layout from './Layout';\nimport { getProducts } from './apiCore';\nimport Card from './Card';\n\nconst Home = () => {\n  const [productsBySell, setProductsBySell] =useState([]);\n  const [productsByArrival, setProductsByArrival] =useState([]);\n  const [error, setError] =useState(false);\n\n  const loadProductBySell = () => {\n    getProducts('sold')\n    .then(data => {\n      if (data.error) {\n        setError(data.error)\n      } else {\n        setProductsBySell(data)\n      }\n    })\n  }\n\n  const loadProductByArrival = () => {\n    getProducts('createdAt')\n    .then(data => {\n      if (data.error) {\n        setError(data.error)\n      } else {\n        setProductsByArrival(data)\n      }\n    })\n  }\n\n  useEffect( () => {\n    loadProductBySell()\n    loadProductByArrival()\n  }, [] );\n\n  return (\n    <Layout title=\"Home Page\" description=\"Node React E-commerce App\" className=\"container-fluid\">\n      <h2 className=\"mb-4\">Best Sellers</h2>\n      <div className=\"row\">\n        { productsBySell.map((product, i) => (\n          <Card key={i} product={product} />\n        ))}\n\n      </div>\n\n      <h2 className=\"mb-4\">New Arrivals</h2>\n      <div className=\"row\">\n        { productsByArrival.map((product, i) => (\n          <Card key={i} product={product} />\n        ))}\n\n      </div>\n    </Layout>\n  )\n}\nexport default Home;\n","/mnt/c/Users/USER/Desktop/nodejs/mern-ecommerce-front/src/auth/Signup.js",[],["93","94"],"/mnt/c/Users/USER/Desktop/nodejs/mern-ecommerce-front/src/Routes.js",[],["95","96"],"/mnt/c/Users/USER/Desktop/nodejs/mern-ecommerce-front/src/config.js",[],["97","98"],"/mnt/c/Users/USER/Desktop/nodejs/mern-ecommerce-front/src/index.js",[],["99","100"],"/mnt/c/Users/USER/Desktop/nodejs/mern-ecommerce-front/src/authBE/index.js",[],"/mnt/c/Users/USER/Desktop/nodejs/mern-ecommerce-front/src/authBE/PrivateRoutes.js",["101"],"import React, { Component } from 'react';\r\nimport { Route, Redirect } from 'react-router-dom';\r\nimport { isAuthenticated } from './index';\r\n\r\n\r\nconst PrivateRoute = ({ component: Component, ...rest }) => (\r\n  <Route\r\n    { ...rest }\r\n    render= { props => isAuthenticated() ? (\r\n      <Component {...props} />\r\n    ) : (\r\n      <Redirect to={{ pathname: '/signin', state: { from: props.location } }} />\r\n    )\r\n   }\r\n  />\r\n)\r\n\r\nexport default PrivateRoute;\r\n","/mnt/c/Users/USER/Desktop/nodejs/mern-ecommerce-front/src/auth/UserDashboard.js",["102"],"import React from 'react';\r\nimport { Link } from 'react-router-dom';\r\nimport Layout from '../core/Layout';\r\nimport { isAuthenticated } from '../authBE';\r\n\r\nconst Dashboard = () => {\r\n  // flaw : The role can be changed in the client end ensure that you authenticate with token\r\n  const { user: { _id, name, email, role } } = isAuthenticated();\r\n\r\n  const userInfo = () => {\r\n    return (\r\n      <div className=\"card mb-5\">\r\n        <h3 className=\"card-header\">User Information</h3>\r\n        <ul className=\"list-group\">\r\n          <li className=\"list-group-item\">{name}</li>\r\n          <li className=\"list-group-item\">{email}</li>\r\n          <li className=\"list-group-item\">{role === 1 ? \"Admin\" : \"Registered User\" }</li>\r\n        </ul>\r\n      </div>\r\n\r\n    )\r\n  }\r\n\r\n  const purchaseHisory = () => {\r\n    return (\r\n      <div className=\"card mb-5\">\r\n        <h3 className=\"card-header\">Purchase History</h3>\r\n        <ul className=\"list-group\">\r\n          <li className=\"list-group-item\">History</li>\r\n        </ul>\r\n      </div>\r\n\r\n    )\r\n  }\r\n\r\n  const userLinks = () => {\r\n    return (\r\n      <div className=\"card\">\r\n        <h4 className=\"card-header\">User Links</h4>\r\n        <ul className=\"list-group\">\r\n          <li className=\"list-group-item\">\r\n            <Link className=\"nav-link\" to=\"/cart\">My Cart</Link>\r\n          </li>\r\n          <li className=\"list-group-item\">\r\n            <Link className=\"nav-link\" to=\"/profile/update\">Update Profile</Link>\r\n          </li>\r\n        </ul>\r\n      </div>\r\n    )\r\n  }\r\n\r\n  return (\r\n    <Layout title=\"Dashboard\" description={`Hello ${name}!`} className=\"container-fluid\">\r\n      <div className=\"row\">\r\n        <div className=\"col-3\">\r\n          {userLinks()}\r\n        </div>\r\n        <div className=\"col-9\">\r\n          {userInfo()}\r\n          {purchaseHisory()}\r\n        </div>\r\n      </div>\r\n    </Layout>\r\n  )\r\n}\r\n\r\nexport default Dashboard;\r\n","/mnt/c/Users/USER/Desktop/nodejs/mern-ecommerce-front/src/authBE/AdminRoutes.js",["103"],"import React, { Component } from 'react';\r\nimport { Route, Redirect } from 'react-router-dom';\r\nimport { isAuthenticated } from './index';\r\n\r\n\r\nconst AdminRoute = ({ component: Component, ...rest }) => (\r\n  <Route\r\n    { ...rest }\r\n    render= { props => isAuthenticated() && isAuthenticated().user.role === 1 ? (\r\n      <Component {...props} />\r\n    ) : (\r\n      <Redirect to={{ pathname: '/signin', state: { from: props.location } }} />\r\n    )\r\n   }\r\n  />\r\n)\r\n\r\nexport default AdminRoute;\r\n","/mnt/c/Users/USER/Desktop/nodejs/mern-ecommerce-front/src/auth/AdminDashboard.js",["104"],"import React from 'react';\r\nimport { Link } from 'react-router-dom';\r\nimport Layout from '../core/Layout';\r\nimport { isAuthenticated } from '../authBE';\r\n\r\nconst AdminDashboard = () => {\r\n  // flaw : The role can be changed in the client end ensure that you authenticate with token\r\n  const { user: { _id, name, email, role } } = isAuthenticated();\r\n\r\n  const adminInfo = () => {\r\n    return (\r\n      <div className=\"card mb-5\">\r\n        <h3 className=\"card-header\">User Information</h3>\r\n        <ul className=\"list-group\">\r\n          <li className=\"list-group-item\">{name}</li>\r\n          <li className=\"list-group-item\">{email}</li>\r\n          <li className=\"list-group-item\">{role === 1 ? \"Admin\" : \"Registered User\" }</li>\r\n        </ul>\r\n      </div>\r\n\r\n    )\r\n  }\r\n\r\n\r\n  const adminLinks = () => {\r\n    return (\r\n      <div className=\"card\">\r\n        <h4 className=\"card-header\">Admin Links</h4>\r\n        <ul className=\"list-group\">\r\n          <li className=\"list-group-item\">\r\n            <Link className=\"nav-link\" to=\"/create/category\">Create category</Link>\r\n          </li>\r\n          <li className=\"list-group-item\">\r\n            <Link className=\"nav-link\" to=\"/create/product\">Create Product</Link>\r\n          </li>\r\n        </ul>\r\n      </div>\r\n    )\r\n  }\r\n\r\n  return (\r\n    <Layout title=\"Admin Dashboard\" description={`Hello ${name}!`} className=\"container-fluid\">\r\n      <div className=\"row\">\r\n        <div className=\"col-3\">\r\n          {adminLinks()}\r\n        </div>\r\n        <div className=\"col-9\">\r\n          {adminInfo()}\r\n        </div>\r\n      </div>\r\n    </Layout>\r\n  )\r\n}\r\n\r\nexport default AdminDashboard;\r\n","/mnt/c/Users/USER/Desktop/nodejs/mern-ecommerce-front/src/admin/AddCategories.js",[],"/mnt/c/Users/USER/Desktop/nodejs/mern-ecommerce-front/src/admin/AddProduct.js",["105","106","107","108","109","110"],"import React, { useState, useEffect } from 'react';\r\nimport Layout from '../core/Layout';\r\nimport { isAuthenticated } from '../authBE';\r\nimport { Link } from 'react-router-dom';\r\nimport { createProduct, getCategories } from './apiAdmin';\r\n\r\nconst AddProduct = () => {\r\n\r\n  const { user , token } = isAuthenticated();\r\n  const [values, setValues] = useState({\r\n    name: \"\",\r\n    description: \"\",\r\n    price: \"\",\r\n    categories: [],\r\n    category: \"\",\r\n    shipping: \"\",\r\n    quantity: \"\",\r\n    photo: \"\",\r\n    loading: false,\r\n    error: \"\",\r\n    createdProduct: \"\",\r\n    redirectToProfile: false,\r\n    formData: \"\"\r\n  })\r\n\r\n  const { name,\r\n    description,\r\n    price,\r\n    categories,\r\n    category,\r\n    shipping,\r\n    quantity,\r\n    photo,\r\n    loading,\r\n    error,\r\n    createdProduct,\r\n    redirectToProfile,\r\n    formData } = values;\r\n\r\n  // loAD CATEGORIES AND SET FORM data\r\n  const init = () => {\r\n    getCategories().then(data => {\r\n      if (data.error) {\r\n        setValues({...values, error: data.error})\r\n      } else {\r\n        setValues({...values, categories: data, formData: new FormData()})\r\n      }\r\n    })\r\n  }\r\n\r\n  useEffect(() => {\r\n    init()\r\n  }, [])\r\n\r\n  const handleChange = key => event => {\r\n    const value = key === 'photo' ? event.target.files[0] : event.target.value\r\n    formData.set(key, value)\r\n    setValues({\r\n      ...values,\r\n      [key]: value\r\n    })\r\n  }\r\n\r\n  const clickSumbit = (event) => {\r\n    setValues({...values, error: '', loading: true})\r\n    event.preventDefault();\r\n\r\n    createProduct(user._id, token, formData)\r\n    .then(data => {\r\n      if(data.error) {\r\n        setValues({...values, error: data.error})\r\n      } else {\r\n        setValues({\r\n          ...values,\r\n          name: '',\r\n          description: '',\r\n          photo: '',\r\n          price: '',\r\n          error: \"\",\r\n          quantity: '',\r\n          loading: false,\r\n          createdProduct: data.name\r\n        })\r\n      }\r\n    })\r\n  };\r\n\r\n  const productForm = () => {\r\n    return (\r\n      <form onSubmit={ clickSumbit } className=\"mb-3\">\r\n        <h4>Post Photo</h4>\r\n        <div className=\"form-group\">\r\n          <label className=\"btn btn btn-secondary\">\r\n            <input type=\"file\" onChange={ handleChange('photo') } name=\"photo\" accept=\"image/*\" />\r\n          </label>\r\n        </div>\r\n        <div className=\"form-group\">\r\n          <label className=\"text-muted\">Name</label>\r\n          <input type=\"text\" className=\"form-control\" value={name} onChange={ handleChange('name') }/>\r\n        </div>\r\n        <div className=\"form-group\">\r\n          <label className=\"text-muted\">Description</label>\r\n          <textarea type=\"text\" className=\"form-control\" value={description} onChange={ handleChange('description') }/>\r\n        </div>\r\n        <div className=\"form-group\">\r\n          <label className=\"text-muted\">Price</label>\r\n          <input type=\"number\" className=\"form-control\" value={price} onChange={ handleChange('price') }/>\r\n        </div>\r\n        <div className=\"form-group\">\r\n          <label className=\"text-muted\">Quantity</label>\r\n          <input type=\"number\" className=\"form-control\" value={quantity} onChange={ handleChange('quantity') }/>\r\n        </div>\r\n        <div className=\"form-group\">\r\n          <label className=\"text-muted\">Category</label>\r\n          <select className=\"form-control\" onChange={ handleChange('category') }>\r\n            <option>Please Select...</option>\r\n            { categories && categories.map(\r\n              (c, i) => (\r\n                <option value={c._id} key={i}>{c.name}</option>\r\n              )\r\n            ) }\r\n          </select>\r\n        </div>\r\n        <div className=\"form-group\">\r\n          <label className=\"text-muted\">Shipping</label>\r\n          <select className=\"form-control\" onChange={ handleChange('shipping') }>\r\n            <option>Please Select...</option>\r\n            <option value=\"0\">No</option>\r\n            <option value=\"1\">Yes</option>\r\n          </select>\r\n        </div>\r\n        <button className=\"btn btn-outline-primary\">Create Product</button>\r\n      </form>\r\n    )\r\n  }\r\n\r\n  const showError = () => (\r\n      <div className=\"alert alert-danger\" style={{ display: error ? \"\" : \"none\" }}>\r\n        { error }\r\n      </div>\r\n  )\r\n\r\n  const showSuccess = () => (\r\n      <div className=\"alert alert-info\" style={{ display: createdProduct ? \"\" : \"none\" }}>\r\n        <h2>{ `${createdProduct} was created successfully!` }</h2>\r\n      </div>\r\n  )\r\n\r\n  const showLoading = () => (\r\n    loading && (\r\n      <div className=\"alert alert-success\">\r\n        <h2>Loading...</h2>\r\n      </div>\r\n    )\r\n  )\r\n\r\n// The reurn page\r\n  return (\r\n\r\n      <Layout title=\"Add a new product\" description={ `Hello ${user.name}, add a product` }>\r\n        <div className=\"row\">\r\n          <div className=\"col-md-8 offset-md-2\">\r\n            { showError() }\r\n            { showSuccess() }\r\n            { showLoading() }\r\n            { productForm() }\r\n          </div>\r\n        </div>\r\n      </Layout>\r\n  )\r\n\r\n};\r\n\r\nexport default AddProduct;\r\n","/mnt/c/Users/USER/Desktop/nodejs/mern-ecommerce-front/src/admin/apiAdmin.js",[],"/mnt/c/Users/USER/Desktop/nodejs/mern-ecommerce-front/src/core/apiCore.js",[],"/mnt/c/Users/USER/Desktop/nodejs/mern-ecommerce-front/src/core/Card.js",[],"/mnt/c/Users/USER/Desktop/nodejs/mern-ecommerce-front/src/core/ShowImage.js",[],"/mnt/c/Users/USER/Desktop/nodejs/mern-ecommerce-front/src/core/Shop.js",["111","112","113","114"],{"ruleId":"115","severity":1,"message":"116","line":9,"column":10,"nodeType":"117","messageId":"118","endLine":9,"endColumn":15},{"ruleId":"119","replacedBy":"120"},{"ruleId":"121","replacedBy":"122"},{"ruleId":"119","replacedBy":"123"},{"ruleId":"121","replacedBy":"124"},{"ruleId":"119","replacedBy":"125"},{"ruleId":"121","replacedBy":"126"},{"ruleId":"119","replacedBy":"127"},{"ruleId":"121","replacedBy":"128"},{"ruleId":"115","severity":1,"message":"129","line":1,"column":17,"nodeType":"117","messageId":"118","endLine":1,"endColumn":26},{"ruleId":"115","severity":1,"message":"130","line":8,"column":19,"nodeType":"117","messageId":"118","endLine":8,"endColum